PREV_IMAGE=`docker images --filter=reference='storybooks-app:*' "before=${LOCAL_TAG}" --format "{{.Repository}}:{{.Tag}}"`
PREV_DOCKER_IMAGE_TAGS=`docker images --filter=reference='${DOCKERHUB_USERNAME}/storybooks-app:*' "before=${REMOTE_TAG}" --format "{{.Repository}}:{{.Tag}}"`

cleanup_local_image() {
    docker rmi PREV_IMAGE || true
    docker images -qf dangling=true | xargs -r docker rmi || true
}

cleanup_local_image_tags() {
    docker rmi PREV_DOCKER_IMAGE_TAGS || true
}